<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>

<script>





    $(document).ready(function () {
        if (document.querySelector("#status")) {
            var script = document.createElement("script");
            script.src = "https://cdnjs.cloudflare.com/ajax/libs/bootstrap-select/1.14.0-beta2/js/bootstrap-select.min.js";
            document.body.appendChild(script);
        }

        if ($('#ordersTable tbody tr').length === 0) {
            $('#ordersTable').hide();  // إخفاء الجدول
            $('#noDataMessage').show(); // عرض رسالة لا توجد بيانات
        }

        // ضبط قيمة افتراضية لحقل رقم التتبع
        var trackingField = $('#trackingNumber');
        if (trackingField.val() === "") {
            trackingField.val("TRK-");
        }

        // التعامل مع الفلاتر عند إرسال النموذج
        $('#filterForm').submit(function (e) {
            e.preventDefault();

            var status = $('#status').val();
            var trackingNumber = trackingField.val().trim();

            if (status === "All") {
                status = "";
            }

            if (trackingNumber && !trackingNumber.startsWith("TRK-")) {
                trackingNumber = "TRK-" + trackingNumber;
                trackingField.val(trackingNumber);
            }

            $.ajax({
                url: '/Home/IndexPartialView',
                type: 'GET',
                data: { status: status, trackingNumber: trackingNumber }, // إرسال الفلتر إلى الـ Controller
                success: function (data) {
                    $('#container').html(data);  // استبدال المحتوى في #container

                    // إعادة تعيين قيمة الـ status
                    $('#status').val(status || "All");

                    // تحقق من وجود بيانات في الجدول بعد التحديث
                    if ($('#ordersTable tbody tr').length === 0) {
                        $('#ordersTable').hide();  // إخفاء الجدول
                        $('#noDataMessage').show(); // عرض رسالة لا توجد بيانات
                    } else {
                        $('#ordersTable').show(); // إظهار الجدول
                        $('#noDataMessage').hide(); // إخفاء رسالة لا توجد بيانات
                    }
                },
                error: function () {
                    alert('حدث خطأ أثناء جلب البيانات. يرجى المحاولة مرة أخرى.');
                }
            });
        });

        // تطبيق الفلاتر عند الضغط على الزر
        $('#applyFilters').click(function () {
            loadOrders();
        });

        // دالة تحميل البيانات
        function loadOrders() {
            var status = $('#status').val();
            var trackingNumber = trackingField.val().trim();

            if (status === "All") {
                status = "";
            }

            if (trackingNumber && !trackingNumber.startsWith("TRK-")) {
                trackingNumber = "TRK-" + trackingNumber;
                trackingField.val(trackingNumber);
            }

            $.ajax({
                url: '/Home/IndexPartialView',
                type: 'GET',
                data: { status: status, trackingNumber: trackingNumber }, // إرسال الفلتر إلى الـ Controller
                success: function (data) {
                    $('#container').html(data);  // استبدال المحتوى في #container
                },
                error: function () {
                    alert('errrror.');
                }
            });
        }
    });

 
    function openETAModal(orderId, currentETA) {
        $('#modal_orderId').val(orderId); // تعبئة OrderId
        $('#modal_eta_input').val(currentETA); // تعبئة ETA الحالية
        $('#etaModal').modal('show'); // عرض المودل
    }

    // عند النقر على زر الحفظ
    $('#save_eta_btn').click(function () {
        var orderId = $('#modal_orderId').val();
        var eta = $('#modal_eta_input').val();

        // إرسال البيانات إلى الخادم باستخدام AJAX
        $.ajax({
            url: '/Home/SaveETA', // URL الخاص بعملية الحفظ
            type: 'POST',
            data: { orderId: orderId, eta: eta },
            success: function (response) {
                if (response.success) {
                    // تحديث القيمة في الجدول
                    $('#eta_display_' + orderId).text(response.eta);

                    // إغلاق المودل بعد حفظ البيانات
                    $('#etaModal').modal('hide');
                } else {
                    alert('Failed to save ETA.');
                }
            },
            error: function () {
