        $.ajax({
            url: '/Home/GetCustomerDetails',
            type: 'GET',
            data: { orderId: orderId },
            success: function (data) {
                // إدراج البيانات في محتوى المودال
                $('#customerDetailsContent').html(data);
                // إظهار المودال
                $('#customerDetailsModal').modal('show');
            },
